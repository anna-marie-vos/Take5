<div class="container">
  <header class="pageTitle">
    <h1 class="Title">
      {{projectData.project_name}} Safety Information
    </h1>
    <nav>
      <a class="link" href="/index">Home</a>
      <a class="link" href="/index/{{projectData.project_id}}">{{projectData.project_name}} Site Information</a>
      <a class="link" href="/index/{{projectData.project_id}}/hazard_log">Project Hazard Log</a>
    </nav>
  </header>
  <div class ="project">
    <h2>
      Project Description
    </h2>
    <table>
      <tr> <th>Headings:</th><th>Desciptions:</th> </tr>
      <tr> <th>Project Name: </th>
        <td><input placeholder="Project Name" type="text" value ="{{projectData.project_name}}"/>
        </td>
      </tr>
      <tr> <th>Location: </th>
        <td><input placeholder="Project Name" type="text" value ="{{projectData.location}}"/>
          </td>
      </tr>
      <tr> <th>SWMS: </th>
        <td><input placeholder="Project Name" type="text" value ="{{projectData.SWMS}}"/>
        </td>
      </tr>
      <tr><th>Important Notices: </th>
        <td><input placeholder="Project Name" type="text" value ="{{projectData.important_Notices}}"/>
        </td>
      </tr>
    </table>
    <input type="submit" name = "commit" value = "submit" />
  </div>

</div>
